// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Project {
  id String @id @default(cuid())
  name String
  technologies String
  description String
  githubLink String
  link String
}

model Knownledge {
  id String @id @default(cuid())
  name String
  image String
  description String
}