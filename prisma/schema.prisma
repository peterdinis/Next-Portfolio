// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Project {
  id          String @id @default(cuid())
  name        String
  description String
  image       String
  githubLink  String
}

model Image {
  id          String @id @default(cuid())
  name        String
  image       String
  description String
}